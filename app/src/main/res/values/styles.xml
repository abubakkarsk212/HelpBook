<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#ffde03</item>
        <item name="colorPrimaryDark">#ffc107</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="noactionbar" parent="Theme.AppCompat.Light.NoActionBar">
        //500 Amber
        <item name="colorPrimaryDark">#ffc107</item>
    </style>
    <style name="titleHeading">
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:background">#FFFFFF</item>
        <item name="android:fontFamily">sans-serif-medium</item>

    </style>
    <style name="headingContact">

        <item name="android:textColor">#3f51b5</item>
        <item name="android:background">#e0e0e0</item>
        <item name="android:textSize">22sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="teztContact">
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">16dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">#64dd17</item>
        <item name="android:textSize">18sp</item>
    </style>



</resources>
